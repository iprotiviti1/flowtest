{
  "properties": {
    "displayName": "When a new email arrives , Send an email",
    "summary": "Test-When a new email arrives , Send an email",
    "description": "Test-When a new email arrives , Send an email",
    "author": {
      "displayName": "Microsoft Flow Community",
      "source": "Community"
    },
    "definition": {
      "$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
      "contentVersion": "1.0.0.0",
      "parameters": {
        "$connections": {
          "defaultValue": {},
          "type": "Object"
        },
        "$authentication": {
          "defaultValue": {},
          "type": "SecureObject"
        }
      },
      "triggers": {
        "When_a_new_email_arrives": {
          "recurrence": {
            "interval": 1,
            "frequency": "Minute"
          },
          "splitOn": "@triggerBody()?['value']",
          "metadata": {
            "flowSystemMetadata": {
              "swaggerOperationId": "OnNewEmail"
            }
          },
          "type": "ApiConnection",
          "inputs": {
            "host": {
              "connection": {
                "name": "@parameters('$connections')['shared_office365']['connectionId']"
              }
            },
            "method": "get",
            "path": "/datasets/@{encodeURIComponent(encodeURIComponent('https://d60cdev01.sharepoint.com/sites/SC80014T00511'))}/tables/@{encodeURIComponent(encodeURIComponent('40c0375b-754d-4d64-8c29-f982a8876454'))}/items",
            "queries": {
              "folderPath": "Inbox",
              "to": "",
              "cc": "",
              "from": "",
              "importance": "Any",
              "fetchOnlyWithAttachment": false,
              "includeAttachments": false
            },
            "authentication": "@parameters('$authentication')"
          }
        }
      },
      "actions": {
        "Send_an_email": {
          "runAfter": {},
          "metadata": {
            "flowSystemMetadata": {
              "swaggerOperationId": "SendEmail"
            }
          },
          "type": "ApiConnection",
          "inputs": {
            "host": {
              "connection": {
                "name": "@parameters('$connections')['shared_office365']['connectionId']"
              }
            },
            "method": "post",
            "body": {
              "To": "",
              "Subject": "Test",
              "Body": "Test",
              "From": "@triggerBody()['Test1']",
              "Cc": "",
              "Bcc": ""
            },
            "path": "/Mail/OnNewEmail",
            "authentication": "@parameters('$authentication')"
          }
        },
        "Send_email": {
          "runAfter": {
            "Send_an_email": [
              "Succeeded"
            ]
          },
          "metadata": {
            "flowSystemMetadata": {
              "swaggerOperationId": "SendEmail"
            }
          },
          "type": "ApiConnection",
          "inputs": {
            "host": {
              "api": {
                "runtimeUrl": "https://flow-apim-msmanaged-na-centralus-01.azure-apim.net/apim/gmail"
              },
              "connection": {
                "name": "@parameters('$connections')['shared_gmail']['connectionId']"
              }
            },
            "method": "post",
            "body": {
              "To": "",
              "Subject": "AA",
              "Body": "AA",
              "Cc": "",
              "Bcc": "",
              "IsHtml": true
            },
            "path": "/Mail/OnNewEmail",
            "authentication": "@parameters('$authentication')"
          }
        }
      },
      "outputs": {}
    },
    "connectionReferences": {
      "shared_office365": {
        "api": {
          "id": "/providers/Microsoft.PowerApps/apis/shared_office365"
        },
        "source": "NotSpecified",
        "connectionNamingMode": "NotSpecified"
      },
      "shared_gmail": {
        "api": {
          "id": "/providers/Microsoft.PowerApps/apis/shared_gmail"
        },
        "source": "NotSpecified",
        "connectionNamingMode": "NotSpecified"
      }
    },
    "categoryNames": [
      "productivity",
      "email",
      "productivity"
    ]
  }
}